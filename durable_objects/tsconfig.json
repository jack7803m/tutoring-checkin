{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "alwaysStrict": true,
    "strict": true,
    "noEmitOnError": true,
    "moduleResolution": "node",
    "outDir": "tscOutDir",
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "types": ["@cloudflare/workers-types", "jest"],
    "rootDir": "src"
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
